{
	"format_version": "1.10.0",
	"particle_effect": {
		"description": {
			"identifier": "andexdb:world_border_red_screen_tint",
			"basic_render_parameters": {
				"material": "particles_blend",
				"texture": "textures/particle/slight_red_screen_border"
			}
		},
		"components": {
			"minecraft:emitter_initialization": {
				"creation_expression": "variable.gradient_duration_a=(variable.gradient_duration_a??(variable.gradient_duration??(variable.particle_lifetime??(variable.max_particle_age??0.5))));variable.die=variable.die??0;",
				"per_update_expression": "variable.die=((variable.emitter_age>0.05)&&(query.distance_from_camera>(variable.max_distance_from_camera??5)))?1:0;"
			},
			"minecraft:emitter_rate_instant": {
				"num_particles": 1
			},
			"minecraft:emitter_lifetime_once": {
				"active_time": "variable.max_emitter_age??0.5"
			},
			"minecraft:emitter_shape_point": {},
			"minecraft:particle_lifetime_expression": {
				"max_lifetime": "variable.max_particle_age??0.5",
				"expiration_expression": "variable.die??0"
			},
			"minecraft:particle_initial_speed": 3,
			"minecraft:particle_appearance_billboard": {
				"size": ["variable.size_x??0.5", "variable.size_y??0.25"],
				"facing_camera_mode": "rotate_xyz",
				"uv": {
					"texture_width": 32,
					"texture_height": 32,
					"uv": [0, 0],
					"uv_size": [32, 32]
				}
			},
			"minecraft:particle_appearance_lighting": {},
			"minecraft:particle_appearance_tinting": {
				"color": {
					"interpolant": "math.mod((v.particle_age / (variable.gradient_duration_a??(variable.max_particle_age??(v.particle_lifetime??0.5)))), 1)",
					"gradient": {
						"0.0": "#78FF0000",
						"0.5": "#FFFF0000",
						"1.0": "#78FF0000"
					}
				}
			}
		}
	}
}