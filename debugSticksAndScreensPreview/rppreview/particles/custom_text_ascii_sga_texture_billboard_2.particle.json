{
	"format_version": "1.10.0",
	"particle_effect": {
		"description": {
			"identifier": "andexdb:custom_text_ascii_sga_texture_billboard_2",
			"basic_render_parameters": {
				"material": "particles_alpha",
				"texture": "font/ascii_sga"
			}
		},
		"components": {
			"minecraft:emitter_local_space": {
				"position": true,
				"rotation": true
			},
			"minecraft:emitter_rate_instant": {
				"num_particles": "(variable.emitter_intensity ?? 1.0) * (variable.emitter_intensity ?? 1.0) * ((variable.emitter_intensity ?? 1.0))"
			},/*
			"minecraft:emitter_lifetime_looping": {
				"active_time": 1
			},*/
			"minecraft:emitter_lifetime_once": {
			  "active_time": 0
			},
			"minecraft:emitter_shape_point": {
				"offset": [ "variable.emitter_offset_x ?? 0.0", "variable.emitter_offset_y ?? 0.0", "variable.emitter_offset_z ?? 0.0" ],
				"direction": [ "variable.emitter_direction_x ?? 0.0", "variable.emitter_direction_y ?? 0.0", "variable.emitter_direction_z ?? 0.0"]},
			"minecraft:particle_lifetime_expression": {
				"max_lifetime": "variable.max_lifetime ?? 2.0"
			},/*
			"minecraft:particle_initial_speed": 0,*//*
			"minecraft:particle_motion_dynamic": {},*/
			"minecraft:particle_appearance_tinting": {
			  "color": [ "variable.color.r", "variable.color.g", "variable.color.b", "variable.color.a" ]
			},
			"minecraft:particle_appearance_billboard": {
				"size": ["(variable.particle_random_1*0) * 0.0375 + (variable.size_x ?? 1)", "(variable.particle_random_1*0) * 0.0375 + (variable.size_y ?? 1)"],
				"facing_camera_mode": "direction_z",
				"direction": {
					"mode": "custom",
					"custom_direction": ["variable.direction_x ?? 0.0", "variable.direction_y ?? 0.0", "variable.direction_z ?? -1.0"]
				},
				"uv": {/*
					"texture_width": 512,
					"texture_height": 1024,*/
					"uv": ["variable.uv_u ?? 0.0", "variable.uv_v ?? 0.0"],
					"uv_size": ["variable.uv_size_u ?? 16.0", "variable.uv_size_v ?? 16.0"]
				}
			},
			"minecraft:particle_appearance_lighting": {}
		}
	}
}