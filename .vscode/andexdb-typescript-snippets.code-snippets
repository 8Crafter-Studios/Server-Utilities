            "\t\t\t\t\t\t\t\t.forceShow(player)",
            "\t\t\t\t\t);",
            "\t\t\t\t\tcurrentParameters = {",
            "\t\t\t\t\t\tplayer,",
            "\t\t\t\t\t\tpagen: Math.max(1, Math.min(numpages, (r.formValues?.[0] as string)?.toNumber() ?? page + 1)) - 1,",
